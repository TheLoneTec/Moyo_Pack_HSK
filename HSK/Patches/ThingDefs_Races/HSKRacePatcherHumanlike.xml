<?xml version="1.0" encoding="UTF-8"?>
<Patch>

	<!-- == BasePawn Replacement == -->
	<Operation Class="PatchOperationAttributeSet">
		<xpath>Defs/AlienRace.ThingDef_AlienRace[defName="Alien_Moyo"]</xpath>
		<attribute>ParentName</attribute>
		<value>SK_BasePawn</value>
	</Operation>
	
	<Operation Class="PatchOperationAttributeAdd">
		<xpath>Defs/PawnKindDef[@Name="MoyoPlayerBasePawnKind"]</xpath>
		<attribute>ParentName</attribute>
		<value>SK_BasePlayerPawnKind</value>
	</Operation>
	
	<Operation Class="PatchOperationAttributeSet">
		<xpath>Defs/PawnKindDef[@Name="BaseMoyoMech"]</xpath>
		<attribute>ParentName</attribute>
		<value>SK_BaseMechanoidKind</value>
	</Operation>
	
</Patch>